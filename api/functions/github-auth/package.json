{
  "name": "github-auth",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn build:babel && cp package.json build && cp ../../yarn.lock build && cd build && yarn --production --mutex network && rm yarn.lock",
    "build:babel": "babel src -d build --extensions '.ts,.js' --ignore '**/*.test.js,**/*.test.ts,**/*.d.ts'",
    "clean": "rm -rf build",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts}'",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "test": "jest"
  },
  "peerDependencies": {
    "aws-sdk": "^2.503.0"
  },
  "devDependencies": {
    "@druideirabr/eslint-config": "^0.0.7",
    "@types/aws-lambda": "^8.10.48",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/pino": "^6.0.0",
    "aws-sdk": "^2.656.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "pino": "^6.2.0"
  }
}
